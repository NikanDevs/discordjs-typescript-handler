{
	"name": "discordjs-typescript-handler",
	"version": "1.0.0",
	"description": "A discordJS handler made with typescript!",
	"main": "index.ts",
	"scripts": {
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"build": "tsc",
		"start": "ts-node src/index.ts",
		"start:prod": "node dist/index.js",
		"register": "ts-node scripts/register.ts",
		"format": "prettier --config .prettierrc.yml 'src' 'scripts' --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NikansWorld/discordjs-typescript-handler.git"
	},
	"keywords": [
		"discordjs",
		"discordjs-handler",
		"handler",
		"ts-handler",
		"discordjs-ts-handler"
	],
	"author": "Nikan's World Developers",
	"license": "ISC",
	"dependencies": {
		"chalk": "^4.1.2",
		"discord.js": "^14.0.3",
		"dotenv": "^16.0.1"
	},
	"devDependencies": {
		"@types/node": "^18.0.6",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	}
}

